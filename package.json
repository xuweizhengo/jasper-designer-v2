{
  "name": "jasper-designer",
  "version": "1.0.0",
  "description": "在线Jasper报表设计器",
  "private": true,
  "workspaces": [
    "frontend"
  ],
  "scripts": {
    "dev": "npm run dev --workspace=frontend",
    "build": "npm run build --workspace=frontend",
    "start": "npm run start --workspace=frontend",
    "lint": "npm run lint --workspace=frontend",
    "test": "npm run test --workspace=frontend",
    "backend": "cd backend && ./mvnw spring-boot:run",
    "setup": "npm install && cd backend && ./mvnw dependency:resolve"
  },
  "keywords": [
    "jasper",
    "report",
    "designer",
    "react",
    "spring-boot"
  ],
  "author": "Jasper Designer Team",
  "license": "MIT",
  "devDependencies": {
    "@types/node": "^20.0.0"
  }
}